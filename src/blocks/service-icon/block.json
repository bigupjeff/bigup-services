{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "bigup-services/service-icon",
	"version": "0.1.0",
	"title": "Service Icon",
	"category": "theme",
	"icon": "smiley",
	"description": "Icon custom field for the 'Service' custom post type",
	"usesContext": [ "postId", "postType", "queryId" ],
	"supports": {
		"html": false
	},
	"textdomain": "bigupweb-service-icon",
	"editorScript": "file:./index.js",
	"attributes": {
		"width": {
			"type": "string",
			"default": "60"
		},
		"height": {
			"type": "string",
			"default": "60"
		}
	}
}