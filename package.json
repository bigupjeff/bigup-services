{
	"name": "bigup-services",
	"version": "0.1.5",
	"description": "A custom 'Services' post type with custom meta fields.",
	"scripts": {
		"build": "webpack --mode production",
		"dev": "webpack --watch --mode development",
		"dist": "bash ./bin/dist.sh",
		"build:plugin": "wp-scripts build",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format ./src",
		"lint:css": "wp-scripts lint-style ./src",
		"lint:js": "wp-scripts lint-js --fix ./src",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"devDependencies": {
		"@wordpress/scripts": "^28.0.0",
		"browser-sync-webpack-plugin": "^2.3.0"
	},
	"dependencies": {
		"@wordpress/block-editor": "^13.0.0",
		"@wordpress/blocks": "^13.0.0",
		"@wordpress/i18n": "^5.0.0",
		"@wordpress/compose": "^7.0.0",
		"@wordpress/plugins": "^7.0.0",
		"@wordpress/server-side-render": "^5.0.0",
		"external-svg-loader": "^1.7.1"
